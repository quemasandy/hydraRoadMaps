{
  "name": "design-patterns-typescript",
  "description": "TypeScript examples for all classic GoF design patterns",
  "author": "Alexander Shvets <neochief@shvetsgroup.com>",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-jest": "^27.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "lint": "eslint \"src/_andyPOC/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:fix": "eslint \"src/_andyPOC/**/*.{ts,tsx}\" --fix",
    "lint:all": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:all:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "codestyle:check": "tslint -c tslint.json -p tsconfig.json",
    "codestyle:fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "test": "jest --no-cache --runInBand",
    "test:watch": "jest --no-cache --runInBand --watch",
    "watch:ts": "node --watch -r ts-node/register"
  },
  "dependencies": {
    "csv-parser": "^3.0.0",
    "fs": "^0.0.1-security",
    "ts-node": "^10.9.2"
  }
}
