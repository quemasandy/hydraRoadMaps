{
  "name": "aws-serverless-typescript-exercises",
  "version": "1.0.0",
  "description": "AWS Serverless exercises with TypeScript",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "deploy": "sam build && sam deploy",
    "deploy:dev": "sam build && sam deploy --config-env dev",
    "deploy:prod": "sam build && sam deploy --config-env prod",
    "local": "sam local start-api",
    "logs": "sam logs --tail",
    "clean": "rm -rf .aws-sam dist node_modules"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.540.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/client-sqs": "^3.540.0",
    "@aws-sdk/client-sns": "^3.540.0",
    "@aws-sdk/client-eventbridge": "^3.540.0",
    "@aws-sdk/client-ses": "^3.540.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.540.0",
    "@aws-sdk/lib-dynamodb": "^3.540.0",
    "@aws-sdk/lib-storage": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.136",
    "@types/node": "^20.11.30",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "aws-sdk-client-mock": "^3.0.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "esbuild": "^0.20.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
