{
  "name": "sql-data-modeling-learning",
  "version": "1.0.0",
  "description": "SQL & Data Modeling learning repository",
  "main": "index.js",
  "scripts": {
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d",
    "db:logs": "docker-compose logs -f postgres",
    "db:psql": "docker exec -it postgres-sql-learning psql -U sqluser -d learning_db",
    "db:backup": "docker exec postgres-sql-learning pg_dump -U sqluser learning_db > backups/backup-$(date +%Y%m%d-%H%M%S).sql",
    "db:restore": "cat backups/latest.sql | docker exec -i postgres-sql-learning psql -U sqluser -d learning_db",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate:create": "node-pg-migrate create",
    "seed": "node scripts/seed.js",
    "test:db": "jest --config jest.config.db.js",
    "format:sql": "sql-formatter -l postgresql -i **/*.sql",
    "lint:sql": "sqlfluff lint sql-scripts/",
    "analyze:slow": "node scripts/analyze-slow-queries.js"
  },
  "keywords": [
    "postgresql",
    "sql",
    "database",
    "data-modeling",
    "learning"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^6.2.2",
    "prettier": "^3.1.0",
    "sql-formatter": "^15.0.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "pg": "^8.11.0",
    "pg-promise": "^11.5.4"
  }
}
